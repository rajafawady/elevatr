{
  "name": "elevatr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:prod": "NODE_ENV=production next build",
    "deploy:check": "npm run lint && npm run build:prod",
    "deploy:validate": "powershell -ExecutionPolicy Bypass -File validate-prod.ps1",
    "deploy:vercel": "vercel --prod",
    "deploy:prep": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "env:setup": "powershell -ExecutionPolicy Bypass -File setup-env.ps1",
    "env:check": "echo 'Checking environment configuration...' && node -e \"console.log('NODE_ENV:', process.env.NODE_ENV); console.log('Firebase Project:', process.env.NEXT_PUBLIC_FIREBASE_PROJECT_ID || 'Not set');\"",
    "health:check": "curl -f http://localhost:3000/api/health || echo 'Health check endpoint not available'"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@tanstack/react-query": "^5.80.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "firebase": "^11.9.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "workbox-webpack-plugin": "^7.3.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
